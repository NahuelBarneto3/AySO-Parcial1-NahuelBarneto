   55  echo n  # Nueva partición
   56  echo p  # Primaria
   57  echo 1  # Número de partición
   58  echo   # Primer sector (predeterminado)
   59  echo +2.5G  # Tamaño de la partición
   60  echo n  # Nueva partición
   61  echo p  # Primaria
   62  echo 2  # Número de partición
   63  echo   # Primer sector (predeterminado)
   64  echo +2.5G  # Tamaño de la partición
   65  echo n  # Nueva partición
   66  echo p  # Primaria
   67  echo 3  # Número de partición
   68  echo   # Primer sector (predeterminado)
   69  echo +2.5G  # Tamaño de la partición
   70  echo n  # Nueva partición
   71  echo e  # Extendida
   72  echo 4  # Número de partición
   73  echo   # Primer sector (predeterminado)
   74  echo   # Último sector (predeterminado)
   75  echo n  # Nueva partición lógica
   76  echo l  # Lógica
   77  echo   # Número de partición (predeterminado)
   78  echo   # Primer sector (predeterminado)
   79  echo +2.5G  # Tamaño de la partición
   80  echo w  # Escribir cambios
   81  ) | fdisk $DISCO
